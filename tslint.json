{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "interface-over-type-literal": false,
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "no-namespace": [false],
        "no-submodule-imports": true
    },
    "rulesDirectory": []
}